{
  "name": "bookify",
  "version": "0.0.0",
  "scripts": {
    "dev": "concurrently \"npm run start:css\" \"npm run start\"",
    "start": "vite",
    "start:css": "tailwindcss -o src/tailwind.css --watch",
    "build": "npm run build:css && vite build",
    "build:css": "tailwindcss -o src/tailwind.css",
    "preview": "vite preview --port 4173"
  },
  "dependencies": {
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^3.0.1",
    "autoprefixer": "^10.4.8",
    "concurrently": "^7.4.0",
    "postcss": "^8.4.16",
    "postcss-nested": "^5.0.6",
    "sass": "^1.54.2",
    "tailwindcss": "^3.1.8",
    "vite": "^3.0.1"
  }
}
